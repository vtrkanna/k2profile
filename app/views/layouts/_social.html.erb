<!-- Social Icons -->
<%= content_tag (:div),:class=>"row social" do %>
    <%= content_tag (:ul),:class=>"social-icons" do %>
        <%= content_tag (:li) do %>
            <%= link_to(image_tag("fb.png",:alt => "Facebook"),"https://www.facebook.com/vtrkanna",:target =>"_blank") %>
            <%= link_to(image_tag("tw.png",:alt => "twitter"),"https://twitter.com/vtrkanna",:target =>"_blank") %>
            <%= link_to(image_tag("go.png",:alt => "google plus"),"https://plus.google.com/u/0/+rajeshkannavtrkanna/about",:target =>"_blank") %>
            <%= link_to(image_tag("pin.png",:alt => "pinterest"),"http://www.pinterest.com/vtrkanna/",:target =>"_blank") %>
            <%= link_to(image_tag("st.png",:alt => "stumbleupon"),"http://www.stumbleupon.com/stumbler/vtrkanna",:target =>"_blank") %>
            <%= link_to(image_tag("dr.png",:alt => "dribbble"),"https://dribbble.com/vtrkanna",:target =>"_blank") %>
        <% end  %>
    <% end  %>
<% end  %>
